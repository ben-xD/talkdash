{
  "$schema": "https://turbo.build/schema.json",
  "globalPassThroughEnv": [],
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": ["dist/**"]
    },
    "frontend#build": {
      "dependsOn": [
        "^build"
      ],
      "dotEnv": [".env"],
      "outputs": ["dist/**"]
    },
    "dev": {
      "persistent": true
    },
    "lint": {},
    "lint:fix": {},
    "clean": {}
  }
}