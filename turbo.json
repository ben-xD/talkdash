{
  "$schema": "https://turbo.build/schema.json",
  "globalPassThroughEnv": [],
  "globalDotEnv": ["frontend/.env"],
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ]
    },
    "frontend#build": {
      "dependsOn": ["^build"],
      "outputs": ["frontend/dist/**"]
    },
    "backend#build": {
      "outputs": ["backend/dist/**"]
    },
    "dev": {
      "persistent": true
    },
    "lint": {},
    "format": {},
    "clean": {}
  }
}