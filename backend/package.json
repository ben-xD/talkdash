{
  "name": "@talkdash/backend",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch --clear-screen=false src/backend.ts",
    "start": "node dist/src/backend.js",
    "deploy": "fly deploy --app talkdash ..",
    "deploy:staging": "fly deploy --app talkdash-staging ..",
    "lint": "prettier --check src && eslint src",
    "lint:fix": "prettier src --write && eslint src --fix",
    "db": "docker-compose up -d db",
    "docker": "docker-compose up -d backend",
    "db:logs": "docker logs talkdash-pg_proxy-1 -f",
    "db:reset": "docker-compose down -v && docker-compose up -d db",
    "db:gen": "tsx node_modules/drizzle-kit/bin.cjs generate:pg",
    "db:gen-broken": "drizzle-kit generate:pg",
    "db:ui": "tsx node_modules/drizzle-kit/bin.cjs studio",
    "clean": "rimraf dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20.8.7",
    "@types/pg": "^8.10.7",
    "@types/uuid": "^9.0.6",
    "@types/ws": "^8.5.8",
    "eslint": "^8.52.0",
    "eslint-config-talkdash": "workspace:*",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@fastify/websocket": "^8.2.0",
    "@lucia-auth/adapter-postgresql": "^2.0.2",
    "@lucia-auth/oauth": "^3.5.0",
    "@neondatabase/serverless": "^0.6.0",
    "@trpc/server": "^10.41.0",
    "bcrypt": "^5.1.1",
    "chalk": "^5.3.0",
    "dotenv": "^16.3.1",
    "drizzle-kit": "^0.20.6",
    "drizzle-orm": "^0.29.1",
    "drizzle-zod": "^0.5.1",
    "fastify": "^4.24.3",
    "lucia": "^2.7.4",
    "openai": "^4.14.1",
    "pg": "^8.11.3",
    "@talkdash/schema": "workspace:*",
    "superjson": "^2.0.0",
    "trpc-panel": "^1.3.4",
    "tsx": "^3.14.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  }
}
